
{
  "entities" : [
    {
      "name" : "person",
      "number" : 100000,
      "attributes" : [
        {
          "name" : "country",
          "type" : "String",
          "generator" : {
            "name" : "org.dama.datasynth.generators.CDFGenerator",
            "requires" : [],
            "init" : ["/dicLocations.txt",1,5," "]
          }
        },
        {
          "name" : "creationDate",
          "type" : "String",
          "generator" : {
            "name" : "org.dama.datasynth.generators.DateGenerator",
            "requires" : [],
            "init" : ["dd-MM-yyyy","01-01-1980","01-01-1990"]
          }
        },
        {
          "name" : "name",
          "type" : "String",
          "generator" : {
            "name" : "org.dama.datasynth.generators.CorrellationGenerator",
            "requires" : ["person.country"],
            "init" : ["/namesByCountry.txt"," "]
          }
        },
        {
          "name" : "id",
          "type" : "Long",
          "generator" : {
            "name" : "org.dama.datasynth.generators.DateBasedIDGenerator",
            "requires" : ["person.creationDate"],
            "init" : ["dd-MM-yyyy"]
          }
        },
        {
          "name" : "email",
          "type" : "String",
          "generator" : {
            "name" : "org.dama.datasynth.generators.MailGenerator",
            "requires" : ["person.name"],
