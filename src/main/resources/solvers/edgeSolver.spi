signature : {
    @A = Edge;
    @A->source.name == @A->target.name;
    @A.direction == 'undirected';
}

var hgen = init('org.dama.datasynth.generators.HashCombiner');
var hdep = union(@A->source.name,@A->correlates.name);
var hattr = map(hgen,hdep);
var table = union(@A->source.name,hattr,@A->sourcecardinality.name);
var sorted = sort(table,1);
var edgegenerator = init('org.dama.datasynth.generators.edgegenerators.UndirectedEdgeGenerator');
@A.name = mappart(edgegenerator,sorted);

